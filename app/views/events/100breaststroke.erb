<div class="content l-content">
	<h1 class="content__header">100 Breaststroke</h1>
	<form class="entryform" method="post" action="/event/times">

	<input type="hidden" name="event" value="100 Breaststroke">

		<ol class="competitors__list">
			
			<% i = 0
			while i < @peoplein100BR.length %>


			<li class="competitors__listitem competitors__listitem--even">
				<p class="competitors__name"><%= @peoplein100BR[i]  %></p><p class="competitors__name"><%= @colleges100BR[i] %> | <%= @confs100BR[i][0] %></p>
				<div class="competitors__time">
					<input class="competitors__textfield" type="text" name="<%= @iDs100BR[i]%>-time" placeholder="time"> 
					<p>sec.</p>
				</div>
			</li>

			<% i+=1
			end %>

			
		</ol>
		<input class="entryform__submit l-right" type="submit" name="Submit">
	</form>
</div>